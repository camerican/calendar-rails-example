<%= form_for @user, html: {method: :patch} do |f| %>
	<%= f.label :name_first, "First Name" %>
	<%= f.text_field :name_first %>
	<%= f.label :name_last, "Last Name" %>
	<%= f.text_field :name_last %>
	<%= f.label :email %>
	<%= f.text_field :email %>
	<%= f.label :username %>
	<%= f.text_field :username %>
	<%= f.submit %>
<% end %>